{
    "name": "xbnz/laravel-multi-ip",
    "description": "Wrapper for several IP resolver APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "A G",
            "email": "fdwnct@gmail.com"
        }
    ],
    "require": {
        "spatie/data-transfer-object": "^3.7",
        "guzzlehttp/guzzle": "^7.4",
        "ext-intl": "*",
        "caseyamcl/guzzle_retry_middleware": "^2.7",
        "kevinrob/guzzle-cache-middleware": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "XbNz\\Resolver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XbNz\\Resolver\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XbNz\\Resolver\\ServiceProviders\\IpServiceProvider",
                "XbNz\\Resolver\\ServiceProviders\\ResolverServiceProvider"
            ],
            "aliases": {
                "Resolver": "XbNz\\Resolver\\Facades\\ResolverFacade"
            }
        }
    }
}
